{
  "mcpServers": {
    "unity-mcp-server": {
      "command": "bash",
      "args": [
        "-lc",
        "UNITY_MCP_MCP_HOST=\"$UNITY_MCP_MCP_HOST\" node mcp-server/bin/unity-mcp-server.js"
      ]
    },
    "context7": {
      "type": "sse",
      "url": "https://mcp.context7.com/sse"
    },
    "markitdown": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "markitdown-mcp"
      ]
    }
  }
}
