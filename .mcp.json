{
  "mcpServers": {
    "unity-mcp-server": {
      "command": "node",
      "args": [
        "mcp-server/bin/unity-mcp-server.js"
      ],
      "env": {
        "UNITY_MCP_MCP_HOST": "${UNITY_MCP_MCP_HOST}",
        "UNITY_MCP_PORT": "${UNITY_MCP_PORT}"
      }
    },
    "context7": {
      "type": "sse",
      "url": "https://mcp.context7.com/sse"
    },
    "markitdown": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "markitdown-mcp"
      ]
    }
  }
}
